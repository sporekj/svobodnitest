{block toolbar-items}
	<li n:foreach="$presenter->importerManager->importers as $type=>$item" class="{if $type == $presenter->type} active{/if}"><a class="ajax" href="{link this type=>$type}">{_$type}</a></li>
{/block}

{block breadcrumb}
