{block #page-content}

{cache [$presenter->routeId, $presenter->lang, $control['pagination']->page], tags => [
	'page-' . $presenter->pageId,
	'route-' . $presenter->routeId,
	'language-' . $presenter->lang,
]}
{control pagination, config => [ variant => '@userlist.route' ]}
{control itemList $control->getItemsBuilder()->orderBy('a.name', 'ASC')->getQuery()->getResult(), config => [ variant => '@userlist.route' ]}
{control pagination, config => [ variant => '@userlist.route' ]}
{/cache}
