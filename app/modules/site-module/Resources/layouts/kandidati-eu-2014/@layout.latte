<div class="col-md-4 visible-md visible-lg">
	<div id="program-menu" class="bs-sidebar hidden-print" style="top: 90px;"  role="complementary" data-offset='90'>

		<ul class="nav bs-sidenav">
			<li><a href="#I">Kandidáti</a>
				<ul class="nav">
				{foreach $items as $name => $item}
				<li><a href="#kandidat-{!substr($item['img'], 0, -4)}">{$iterator->counter}.&nbsp;{!$name}</a></li>
				{/foreach}
				</ul>
			</li>
		</ul>
	</div>
</div>

<script type="application/javascript">
	$(function(){
		var offset = 90;

		$('#program-menu').affix({
			offset: {
				top:  250
			}
		});
		$('body').scrollspy({
			target: '#program-menu',
			offset: 100
		});
		$('#program-menu li a, .col-md-8 a').on('click', function(event) {
			if ($(this).attr('href').substr(0, 1) == '#') {
				event.preventDefault();
				var hash = this.hash;
				$('html, body').animate({
					scrollTop: $(this.hash).offset().top - offset
				}, 300, function () {
					window.location.hash = hash;
				});
			}
		});
	});
</script>